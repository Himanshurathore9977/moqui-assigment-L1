<?xml version="1.0" encoding="UTF-8"?>
    <entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-2.1.xsd">
        <entity entity-name="Employee" package="Employee">
            <field name="EmployeeId" type="id" is-pk="true"/>
            <field name="EmployeeName" type="text-medium"/>
            <field name="DateOfBirth" type="date-time"/>
            <field name ="salary" type="currency-amount"/>
            <field name ="ContactNo" type="number-amount"/>
            <field name="Designation" type="text-medium"/>
        </entity>
    </entities>
